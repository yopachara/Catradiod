<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    android:id="@+id/my_tweet_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:twittersdk="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="com.yopachara.catradiod.activities.TweetsTimelineDetailActivity">
    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/swipeRefreshLayoutDetail"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ListView android:id="@+id/timeline_list_detail"
                  android:layout_width="match_parent"
                  android:layout_height="match_parent"
                  android:divider="#e1e8ed"
                  android:dividerHeight="1dp"/>
    </android.support.v4.widget.SwipeRefreshLayout>

</FrameLayout>
